root = true

[*]
charset = utf-8
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = false
indent_style = space
indent_size = 4

[*.el]
charset = utf-8
end_of_line = crlf
trim_trailing_whitespace = true
insert_final_newline = true
indent_size = 4
indent_style = space



[*.cpp]
indent_style = space
indent_size = 4
tab_width = 4

[*.c]
indent_style = tab
indent_size = 2
tab_width = 2

[*.php]
indent_style = space
indent_size = 4
tab_width = 4